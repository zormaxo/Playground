{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "webapi-route": {
        "ClusterId": "webapi-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "webapi-cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "webapi1-destination": {
            "Address": "http://webapi1:8080/"
          },
          "webapi2-destination": {
            "Address": "http://webapi2:8080/"
          },
          "webapi3-destination": {
            "Address": "http://webapi3:8080/"
          }
        }
      }
    }
  }
}
