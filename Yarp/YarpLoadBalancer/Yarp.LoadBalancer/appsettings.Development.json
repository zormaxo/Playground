{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "webapi1-route": {
        "ClusterId": "webapi1-cluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "webapi1-cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "webapi1-destination": {
            "Address": "http://localhost:7072/"
          },
          "webapi2-destination": {
            "Address": "http://localhost:7073/"
          },
          "webapi3-destination": {
            "Address": "http://localhost:7074/"
          }
        }
      }
    }
  }
}
