{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "default": {
        "ClusterId": "backend",
        "Match": {
          "Path": "/{**catchall}"
        }
      }
    },
    "Clusters": {
      "backend": {
        "Destinations": {
          "destination1": {
            "Address": "http://hello.api:8080/"
          }
        }
      }
    }
  }
}
